{
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "off",
  "window.zoomLevel": 0,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "keyboard.dispatch": "keyCode",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.columnSelection": false,
  "editor.minimap.enabled": true,
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "solid",
  "vim.cursorStylePerMode.insert": "block",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.cursorStylePerMode.visual": "line",
  "vim.cursorStylePerMode.visualblock": "line",
  "vim.cursorStylePerMode.visualline": "line",
  "vim.camelCaseMotion.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.leader": ",",
  "vim.statusBarColorControl": true,
  "vim.vimrc.path": "$HOME/.config/nvim/init.vim",
  "vim.vimrc.enable": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#0E6925",
    "statusBar.noFolderBackground": "#0E6925",
    "statusBar.debuggingBackground": "#0E6925"
  },
  "workbench.colorTheme": "Dracula Soft",
  "workbench.activityBar.visible": true,
  "vim.foldfix": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#0E6925",
  "vim.statusBarColors.insert": "#006C83",
  "vim.statusBarColors.visual": "#B05B00",
  "vim.statusBarColors.visualline": "#C16E14",
  "vim.statusBarColors.replace": "#006C83",
  "vim.statusBarColors.commandlineinprogress": "#903067",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC"
}